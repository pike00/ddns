{
  "provider": "Cloudflare",
  "login_token": "<Cloudflare Token>",
  "domains": [{
      "domain_name": "<domain>",
      "sub_domains": ["<subdomain>"]
    }],
  "resolver": "1.1.1.1",
  "ip_url": "https://api.ipify.org",
  "ip_type": "IPv4",
  "interval": 300,
  "socks5_proxy": "",
  "notify": {
    "pushover": {
      "enabled": true,
      "token": "<token url>",
      "user": "<user>",
      "message_template": "Domain <b>{{ .Domain }}</b> is updated to {{ .CurrentIP }}",
      "device": "",
      "title": "<domain> IP Changed",
      "priority": 0,
      "html": 1
    }
  }
}
